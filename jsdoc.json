{
  "tags": {
    "allowUnknownTags": ["optional"]
  },
  "plugins": [
      "node_modules/better-docs/typescript"
  ],
  "source": {
      "includePattern": "\\.(jsx|js|ts|tsx)$"
  },
  "opts": {
    "template": "node_modules/better-docs",
    "destination": "./docs",
    "readme": "docs-src/readme.md",
    "tutorials": "./docs-src/tutorials"
  },
  "templates": {
    "default": {
      "staticFiles": {
        "include": [
            "./docs-src"
        ]
      }
    },
    "better-docs": {
      "name": "PDF.js Express Rest Utility API",
      "title": "PDF.js Express Utils",
      "css": "docs.css",
      "hideGenerator": false
    }
  }
}